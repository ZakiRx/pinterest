{% extends 'base.html.twig' %}

{% block title 'Edit Pin' %}

{% block body %}
<h2 class="text-center mb-3"> Edit Pin</h2>

    {{ form_start(form) }}
    <input type="hidden" name="_token" value="{{ csrf_token("edit_pin"~pin.id) }}">
    {{ form_rest(form) }}
     <button type="submit" class="btn btn-primary">Edit</button>
    {{ form_end(form) }}
{% endblock %}